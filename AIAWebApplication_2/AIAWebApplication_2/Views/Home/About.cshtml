@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>

<script>
    $(document).ready(function () {
        $("#about").toggleClass("active");
        $("#main-menu li").click(function () {
            $(this).toggleClass("active");
        })
    });
</script>